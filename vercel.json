{
  "functions": {
    "api/**/*.php": { "runtime": "vercel-php@0.7.3" }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/api/static.php?file=$1"
    },
    {
      "src": "/favicon\\.ico",
      "dest": "/api/static.php?file=favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}
